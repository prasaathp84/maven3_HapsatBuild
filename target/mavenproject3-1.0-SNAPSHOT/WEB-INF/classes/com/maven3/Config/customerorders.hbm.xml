<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
  
 <hibernate-mapping>  
  <class name="com.maven3.Database.CustomerOrders" table="CustomerOrders">  
    <id name="OrderNo">  
     <generator class="assigned"></generator>  
    </id>  
    <property name="InvoiceDate"></property>  
    <property name="InvoiceNumber"></property>  
    <property name="DocTotal"></property>  
    <property name="CustomerName"></property>  
    <property name="CustomerEmail"></property>  
    <property name="CustomerPhoneNumber"></property>  
    <property name="ItemCode"></property>  
    <property name="ItemDescription"></property>     
   </class>  
 </hibernate-mapping>  